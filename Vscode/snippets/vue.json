{
	// Place your snippets for vue here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Vue Template Style-scss": {
		"prefix": "v2-template-scss",
		"body": [
			"<template>",
			"\t$1",
			"</template>",
			"",
			"<script>",
			"\t$2",
			"</script>",
			"",
			"<style lang=\"scss\">",
			"\t$3",
			"</style>"
		],
		"description": "Vue Template Script-setup-ts Style-scss-scoped"
	},
	"Vue uni-app Template Style-scss": {
		"prefix": "v2-uni-template-scss",
		"body": [
			"<template>",
			"<view class=\"container\">",
			"\t$1",
			"</view>",
			"</template>",
			"",
			"<script>",
			"export default {",
			"data() {",
			"return {",
			"\t$2",
			"};",
			"}",
			"}",
			"</script>",
			"",
			"<style lang=\"scss\">",
			"\t$3",
			"</style>",
		],
		"description": "Vue Template Script-setup-ts Style-scss-scoped"
	},
	"Vue Template Script-setup-ts Style-scss-scoped": {
		"prefix": "v3-ts-setup-scss-scoped",
		"body": [
			"<template>",
			"\t$1",
			"</template>",
			"",
			"<script lang=\"ts\" setup>",
			"\t$2",
			"</script>",
			"",
			"<style lang=\"scss\" scoped>",
			"\t$3",
			"</style>"
		],
		"description": "Vue Template Script-setup-ts Style-scss-scoped"
	},
	"Vue Template Script-setup-ts Style-less-scoped": {
		"prefix": "v3-ts-setup-less-scoped",
		"body": [
			"<template>",
			"\t$1",
			"</template>",
			"",
			"<script lang=\"ts\" setup>",
			"\t$2",
			"</script>",
			"",
			"<style lang=\"less\" scoped>",
			"\t$3",
			"</style>"
		],
		"description": "Vue Template Script-setup-ts Style-less-scoped"
	},
}
